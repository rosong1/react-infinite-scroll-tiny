{
  "name": "react-infinite-scroll-tiny",
  "version": "1.0.0",
  "description": "A tiny Infinite Scroll component in react like react-infinite-scroll-component",
  "main": "./dist/react-infinite-scroll-tiny.cjs",
  "module": "./dist/react-infinite-scroll-tiny.mjs",
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-infinite-scroll-tiny.cjs",
      "import": "./dist/react-infinite-scroll-tiny.mjs"
    }
  },
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rosong1/react-infinite-scroll-tiny.git"
  },
  "keywords": [
    "react",
    "typescript",
    "infinite-scroll",
    "vite",
    "vitest"
  ],
  "author": {
    "name": "rosong",
    "email": "rousong111@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rosong1/react-infinite-scroll-tiny/issues"
  },
  "homepage": "https://github.com/rosong1/react-infinite-scroll-tiny#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.3",
    "jsdom": "^20.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.16.0"
  }
